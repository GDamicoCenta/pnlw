{
  "compilerOptions": {
    "typeRoots": ["./node_modules/@types", "./declarations"],
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "noUnusedParameters": false,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
   
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/_app.js",
    "jest.config.ts",
    "app/[lang]/(plataforma)/mensajeria/components/hermes-table",
    "app/[lang]/(plataforma)/compliance/(onboarding)/legajo/sections/utils"
, "app/api/esco/searchCodAgente"  ],
  "exclude": ["node_modules"]
}
